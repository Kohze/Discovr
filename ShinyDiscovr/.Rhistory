shiny::runApp()
library(shiny)
shinyUI(fluidPage(
# Application title
titlePanel("Discovr Shiny App"),
# Sidebar with a slider input for number of bins
sidebarLayout(
sidebarPanel(
sliderInput("bins",
"Number of bins:",
min = 1,
max = 50,
value = 40)
),
# Show a plot of the generated distribution
mainPanel(
plotOutput("distPlot")
)
)
))
runApp()
